{
  "schemaVersion": 1,
  "id": "fab",
  "version": "${version}",

  "name": "Foodables",
  "accessWidener": "fab.accesswidener",
  "description": "This Mod adds all your delicious favourites to minecraft!",
  "authors": [
    "TRUTH"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/fab/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "truth.foodables.Foodables"
    ],
    "client": [
      {
        "value": "truth.foodables.FoodablesClient"
      }
    ]
  },
  "mixins": [
    "fab.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
