  "authors": ["TRUTH"],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/foodables-fabric",
    "sources": "https://github.com/CanINotUseTruth/Foodables",
    "issues": "https://github.com/CanINotUseTruth/Foodables/issues"
  },

  "license": "MIT",
